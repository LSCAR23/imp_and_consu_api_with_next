generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  body      String
  createdAt DateTime @default(now())
}

model User {
  id              Int      @id @default(autoincrement())
  email           String
  userName        String   @unique
  password        String
  twoFactorSecret String?
  createdAt       DateTime @default(now())
}